<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<TabPane prefHeight="500.0" prefWidth="700.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
        <tabs>
            <Tab text="List of Students">
                <content>
                    <TableView prefHeight="200.0" prefWidth="200.0">
                        <columns>
                            <TableColumn prefWidth="162.0" text="Firstname" />
                            <TableColumn minWidth="0.0" prefWidth="182.0" text="Lastname" />
                            <TableColumn minWidth="0.0" prefWidth="208.0" text="City" />
                            <TableColumn minWidth="0.0" prefWidth="148.0" text="Average grade" />
                        </columns>
                    </TableView>
                </content>
            </Tab>
            <Tab text="List of Courses">
                <content>
                    <TableView prefHeight="200.0" prefWidth="200.0">
                        <columns>
                            <TableColumn prefWidth="195.0" text="Name" />
                            <TableColumn prefWidth="191.0" text="Semester" />
                            <TableColumn prefWidth="173.0" text="Teacher" />
                            <TableColumn prefWidth="141.0" text="Name acronym" />
                        </columns>
                    </TableView>
                </content>
            </Tab>
            <Tab text="Student info">
                <content>
                    <BorderPane prefHeight="200.0" prefWidth="200.0">
                        <top>
                            <Label text="Select student to see courses and grades" BorderPane.alignment="CENTER">
                                <font>
                                    <Font size="30.0" />
                                </font>
                                <BorderPane.margin>
                                    <Insets top="5.0" />
                                </BorderPane.margin>
                            </Label>
                        </top>
                        <bottom>
                            <Button mnemonicParsing="false" text="EXIT" BorderPane.alignment="BOTTOM_RIGHT">
                                <BorderPane.margin>
                                    <Insets bottom="10.0" right="15.0" />
                                </BorderPane.margin>
                            </Button>
                        </bottom>
                        <center>
                            <GridPane BorderPane.alignment="CENTER">
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="344.0" minWidth="10.0" prefWidth="229.0" />
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="512.3333028157551" minWidth="10.0" prefWidth="473.33330281575513" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="471.0" minWidth="10.0" prefWidth="155.3333638509115" />
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                </rowConstraints>
                                <children>
                                    <ComboBox fx:id="comboBoxStudents" prefHeight="39.0" prefWidth="395.0" GridPane.columnIndex="1" />
                                    <Label alignment="CENTER_RIGHT" prefHeight="91.0" prefWidth="238.0" text="Choose a student:">
                                        <font>
                                            <Font size="20.0" />
                                        </font>
                                        <GridPane.margin>
                                            <Insets right="10.0" />
                                        </GridPane.margin>
                                    </Label>
                                    <Label fx:id="LblStudentInfoName" alignment="CENTER" prefHeight="82.0" prefWidth="698.0" text="STUDENT NAME (STUDENT ID)" GridPane.columnSpan="2" GridPane.rowIndex="1" />
                                    <Label fx:id="LblStudentInfoCourse1" alignment="CENTER_RIGHT" prefHeight="95.0" prefWidth="252.0" text="COURSE 1" GridPane.rowIndex="2">
                                        <GridPane.margin>
                                            <Insets right="10.0" />
                                        </GridPane.margin>
                                    </Label>
                                    <Label fx:id="LblStudentInfoCourse2" alignment="CENTER_RIGHT" prefHeight="83.0" prefWidth="250.0" text="COURSE 2" GridPane.rowIndex="3">
                                        <GridPane.margin>
                                            <Insets right="10.0" />
                                        </GridPane.margin>
                                    </Label>
                                    <Label alignment="CENTER_RIGHT" prefHeight="114.0" prefWidth="251.0" text="Average grade:" GridPane.rowIndex="4">
                                        <GridPane.margin>
                                            <Insets right="10.0" />
                                        </GridPane.margin>
                                    </Label>
                                    <Label fx:id="LblStudentInfoGrade1" prefHeight="101.0" prefWidth="510.0" text="Grade" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                    <Label fx:id="LblStudentInfoGrade2" prefHeight="77.0" prefWidth="476.0" text="Grade" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                                    <Label fx:id="LblStudentInfoAvgGrade" prefHeight="71.0" prefWidth="471.0" text="Average grade" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                        <Button mnemonicParsing="false" onAction="#getGrade" text="View" GridPane.columnIndex="2" />
                                </children>
                            </GridPane>
                        </center>
                    </BorderPane>
                </content>
            </Tab>
            <Tab text="Course info">
                <content>
                    <BorderPane prefHeight="200.0" prefWidth="200.0">
                        <center>
                            <GridPane BorderPane.alignment="CENTER">
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="341.0" minWidth="10.0" prefWidth="221.0" />
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="479.0" minWidth="10.0" prefWidth="479.0" />
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                </rowConstraints>
                                <children>
                                    <ComboBox fx:id="comboBoxCourses" prefHeight="39.0" prefWidth="443.0" GridPane.columnIndex="1" />
                                    <Label alignment="CENTER_RIGHT" prefHeight="128.0" prefWidth="243.0" text="Choose a course:">
                                        <GridPane.margin>
                                            <Insets right="10.0" />
                                        </GridPane.margin>
                                        <font>
                                            <Font size="20.0" />
                                        </font>
                                    </Label>
                                    <Label fx:id="LblCourseInfoName" alignment="CENTER" prefHeight="110.0" prefWidth="700.0" text="COURSE NAME (ACRONYM)" GridPane.columnSpan="2" GridPane.rowIndex="1" />
                                    <Label alignment="CENTER_RIGHT" prefHeight="93.0" prefWidth="221.0" text="Teacher:" GridPane.rowIndex="2">
                                        <GridPane.margin>
                                            <Insets right="10.0" />
                                        </GridPane.margin>
                                    </Label>
                                    <Label alignment="CENTER_RIGHT" prefHeight="89.0" prefWidth="227.0" text="Average grade:" GridPane.rowIndex="3">
                                        <GridPane.margin>
                                            <Insets right="10.0" />
                                        </GridPane.margin>
                                    </Label>
                                    <Label fx:id="LblCourseInfoTeacher" prefHeight="108.0" prefWidth="476.0" text="TEACHER NAME" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                    <Label fx:id="LblCourseInfoAvgGrade" prefHeight="85.0" prefWidth="480.0" text="AVERAGE GRADE" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                                </children>
                            </GridPane>
                        </center>
                        <top>
                            <Label text="Select course to see information" BorderPane.alignment="CENTER">
                                <font>
                                    <Font size="30.0" />
                                </font>
                                <BorderPane.margin>
                                    <Insets top="5.0" />
                                </BorderPane.margin>
                            </Label>
                        </top>
                        <bottom>
                            <Button mnemonicParsing="false" text="EXIT" BorderPane.alignment="BOTTOM_RIGHT">
                                <BorderPane.margin>
                                    <Insets bottom="10.0" right="15.0" />
                                </BorderPane.margin>
                            </Button>
                        </bottom>
                    </BorderPane>
                </content>
            </Tab>
        </tabs>
    </TabPane>
